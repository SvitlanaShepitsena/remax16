@import 'nib'

.logo-container
  display block
  background: url("https://s3-us-west-2.amazonaws.com/remax1stclass/brokers/logos/company-logo.png") 0 0 no-repeat;
  position absolute
  width 96px
  height 96px
  background-size 80px 80px
  background-position center
  left 16px
  top 8px
  background-color white

.logo-link
  height auto
  max-width 80px

.logo-img
  width 100%
  max-width 70px
  height auto

.logo-img-cell
  width 100%
  max-width 35px
  height auto

.head-ad
  &:hover
  &:active
  &:focus
    text-decoration none
    color #757575
  @media screen and (max-width: 992px)
    font-size 10px

.sv-cell-13
  @media screen and (max-width: 992px)
    font-size 13px !important

.sv-nav-link-responsive
  @media screen and (max-width: 350px) {
    font-size 11px
  }
  @media screen and (max-width: 1050px) {
    font-size 12px
  }

